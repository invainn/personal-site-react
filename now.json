{
	"name": "personal",
	"alias": "antbnt.me",
    "version": 2,
    "alias": "antbnt.me",
    "builds": [
        { "src": "frontend/next.config.js", "use": "@now/next" }
    ],
    "env": {
        "SITE_URL": "http://localhost:1337"
    },
    "build": {
        "env": {
            "SITE_URL": "@site_url"
        }
    },
    "routes": [
        { "src": "/post/(.*)", "methods": ["GET"], "dest": "/frontend/post?id=$1" },
        { "src": "/(.*)", "methods": ["GET"] ,"dest": "/frontend/$1" }
    ]
}
